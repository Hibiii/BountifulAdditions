{
  "schemaVersion": 1,
  "id": "bountifuladditions",
  "version": "${version}",

  "name": "BountifulAdditions",
  "description": "This mod adds small quality-of-life enhancements that are seem commonly in mods for modern versions or are integrated in the vanilla game, but are missing in 1.8.9. No gameplay is changed.",
  "authors": [
    "Hibi"
  ],
  "contact": {
    "homepage": "https://legacyfabric.net/",
    "sources": "https://github.com/Legacy-Fabric/fabric-example-mod"
  },

  "license": "Unlicense",
  "icon": "assets/bountifuladditions/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "hibi.bountifuladditions.Init"
    ]
  },
  "mixins": [
    "bountifuladditions.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.9.0",
    "minecraft": "1.8.9"
  }
}
